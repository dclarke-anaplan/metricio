{
  "name": "metricio",
  "version": "1.2.0",
  "description": "dashboard for displaying metrics easily",
  "main": "app.js",
  "scripts": {
    "start": "SECRET_SOURCE=local babel-node app.js",
    "start-aws": "aws-cli exec $aws-project-name -- babel-node app.js",
    "buildProdDist": "rm -rf dist/ && npx babel-node extractJobs.js && NODE_ENV=production webpack -p --config webpack.config.server.babel.js && NODE_ENV=production webpack -p --config webpack.config.babel.js",
    "startProd": "NODE_ENV=production node dist/server/app.js",
    "test": "ava test -v",
    "lint": "eslint .",
    "execute-job": "SECRET_SOURCE=local babel-node src/libs/execute-job.js",
    "execute-job-aws": "aws-okta exec $aws-project-name -- babel-node src/libs/execute-job.js"
  },
  "author": {
    "name": "Danny Croft",
    "email": "os@dannycroft.co.uk",
    "url": "https://www.dannycroft.co.uk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dannycroft/metricio"
  },
  "license": "MIT",
  "//": [
    "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!",
    "!!!! READ THIS BEFORE ADDING NEW DEPENDENCIES !!!!",
    "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!",
    "Ensure that any dependencies using by your jobs or any of the code running in the backend are added to the 'dependencies' section below.",
    "Any dependencies used by widgets (or anything frontend related) can be placed in the 'devDependencies' section as they get pre-bundled with source code",
    "before being built into the Docker image"
  ],
  "dependencies": {
    "@kubernetes/client-node": "^0.11.0",
    "aws-sdk": "^2.605.0",
    "connect-redis": "^3.3.3",
    "csv-parse": "^4.8.5",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "ical": "^0.6.0",
    "ioredis": "^3.2.2",
    "ipaddr.js": "^1.9.1",
    "mime": "^2.4.4",
    "node-html-parser": "^1.1.18",
    "node-resque": "^5.3.0",
    "node-schedule": "^1.3.0",
    "p-event": "^2.3.1",
    "qs": "^6.5.2",
    "redis": "^2.8.0",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "require-all": "^2.2.0",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.8.1",
    "webpack-dev-middleware": "^3.1.3",
    "xml-js": "^1.6.11",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@material-ui/core": "^4.1.0",
    "@material-ui/icons": "^4.1.0",
    "ava": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-resolver": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.4",
    "css-loader": "^2.1.1",
    "csshake": "^1.5.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.10.0",
    "mock-socket": "^7.1.0",
    "moment": "^2.22.1",
    "moment-business-days": "^1.1.3",
    "moment-range": "^4.0.2",
    "node-sass": "^4.9.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.1",
    "proxyquire": "^2.0.1",
    "react": "^16.3.2",
    "react-countdown-now": "^2.1.1",
    "react-dom": "^16.3.2",
    "react-sparklines": "^1.7.0",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.6",
    "style-loader": "^0.21.0",
    "victory": "^0.24.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "ava": {
    "files": [
      "./test/**/*.test.js"
    ],
    "source": [
      "./test/**/*.{js,jsx}"
    ],
    "concurrency": 5,
    "failFast": true,
    "require": [
      "babel-register",
      "ignore-styles",
      "./test/helpers/browser.js",
      "./test/helpers/enzyme.js"
    ],
    "babel": "inherit"
  }
}
